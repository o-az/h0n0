{
  "name": "h0n0",
  "version": "0.0.0",
  "type": "module",
  "repository": "github:o-az/h0n0",
  "imports": {
    "#*": "./src/*",
    "#package.json": "./package.json",
    "#wrangler.json": "./wrangler.json"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "NODE_ENV='production' wrangler --config='wrangler.json' deploy",
    "types": "wrangler types --env-interface CloudflareBindings",
    "check": "biome check . --write --unsafe",
    "check:types": "bun tsgo --project tsconfig.json --noEmit",
    "up": "bun x taze@latest --recursive --update major"
  },
  "dependencies": {
    "hono": "^4.11.7"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.14",
    "@cloudflare/vite-plugin": "^1.23.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.3.8",
    "@types/node": "^25.2.0",
    "@typescript/native-preview": "^7.0.0-dev.20260204.1",
    "oxlint": "^1.43.0",
    "oxlint-tsgolint": "^0.11.4",
    "typescript": "^5.9.3",
    "vite": "^8.0.0-beta.12",
    "wrangler": "^4.62.0"
  },
  "license": "MIT",
  "packageManager": "bun@1.3.8"
}
