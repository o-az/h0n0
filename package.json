{
  "name": "h0n0",
  "version": "0.0.0",
  "type": "module",
  "repository": "github:o-az/h0n0",
  "imports": {
    "#*": "./src/*",
    "#package.json": "./package.json",
    "#wrangler.json": "./wrangler.json"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "deploy": "wrangler --config='wrangler.json' deploy",
    "types": "wrangler types --env-interface CloudflareBindings",
    "posttypes": "bun check && bun check",
    "format": "biome format . --write",
    "lint": "biome lint . --write --unsafe",
    "check": "biome check . --write --unsafe",
    "pretypecheck": "bun types",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "up": "bun x taze@latest --recursive --update major"
  },
  "dependencies": {
    "hono": "^4.9.7",
    "vite": "npm:rolldown-vite@latest"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@cloudflare/vite-plugin": "^1.13.1",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/bun": "^1.2.22",
    "@types/node": "^24.4.0",
    "typescript": "^5.9.2",
    "wrangler": "^4.37.0"
  },
  "license": "MIT",
  "packageManager": "bun@1.2.22"
}
